  "scripts": {
    "dev": "concurrently \"npm:api:dev\" \"npm:app:dev\" -n api,app",
    "api:dev": "cross-env NODE_ENV=development nodemon",
    "api:prod": "node_modules/.bin/cross-env NODE_ENV=production ts-node --files ./src/api/index.ts",
    "app:dev": "webpack-dev-server --config webpack.config.dev.js",
    "app:prod": "webpack --config webpack.config.prod.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "make:migration": "node_modules/.bin/sequelize migration:generate --name",
    "test": "cross-env NODE_ENV=test ts-mocha src/tests/**/*.test.ts",
    "deploy": "gh-pages -d dist"
  },
